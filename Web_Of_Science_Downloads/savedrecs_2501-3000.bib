@inproceedings{ ISI:000381620100009,
Author = {Jiang, Hong and Chen, Shuqiao and Hu, Hongchao and Zhang, Mingming},
Book-Group-Author = {{IEEE}},
Title = {{Superpoint-Based Detection Against Distributed Denial of Service (DDoS)
   Flooding Attacks}},
Booktitle = {{2015 IEEE 21ST INTERNATIONAL WORKSHOP ON LOCAL \& METROPOLITAN AREA
   NETWORKS (LANMAN)}},
Series = {{IEEE International Workshop on Local and Metropolitan Area Networks}},
Year = {{2015}},
Note = {{21st IEEE International Workshop on Local and Metropolitan Area Networks
   (LANMAN), Beijing, PEOPLES R CHINA, APR 22-24, 2015}},
Organization = {{IEEE}},
Abstract = {{DDoS flooding attack is a critical threat to the normal operation of
   network. However, current feature-based detection methods are cheated by
   hackers easily and most of these mechanisms do not differentiate between
   DDoS flooding attacks and legitimate random flash crowds with feature
   independent and location extended. To address the challenges, we propose
   a two-stage detection strategy by combining superpoints and flow
   similarity measurement. To locate the suspicious flows, polymerization
   degree of destination superpoints is introduced in a moving time window
   mechanism. Based on the suspicious flows, a sliding-detection algorithm
   is presented for distinguishing flooding attacks from flash crowds with
   similarity metrics. Computer simulation results indicate that our
   detection approach can detect DDoS flooding attacks efficiently and
   Total Variation Distance (TVD) is the most suitable metric for
   discriminating DDoS flooding attack flows from flash crowds. Built on
   flow arrivals, the proposed mechanism is practical for the attack
   detection on high speed links.}},
Publisher = {{IEEE}},
Address = {{345 E 47TH ST, NEW YORK, NY 10017 USA}},
Type = {{Proceedings Paper}},
