@inproceedings{ ISI:000427533900048,
Author = {Khan, Talha and Kadir, Kushairy and Alam, Muhammad and Shahid, Zeeshan
   and Mazliham, Ms},
Book-Group-Author = {{IEEE}},
Title = {{Geomagnetic Field Measurement at Earth Surface: Flash Flood Forecasting
   using Tesla Meter}},
Booktitle = {{2017 INTERNATIONAL CONFERENCE ON ENGINEERING TECHNOLOGY AND
   TECHNOPRENEURSHIP (ICE2T)}},
Year = {{2017}},
Note = {{International Conference on Engineering Technology and Technopreneurship
   (ICE2T), Kuala Lumpur, MALAYSIA, SEP 18-20, 2017}},
Abstract = {{Flash floods can be regarded as an apical natural hazard in many
   countries that eminently affect the safety of the human lives, precious
   belongings, livestock and agricultural farming land. Variety of
   techniques like engineering (construction of dams and reservoirs) and
   non engineering (prior investigation of flash floods) has been advanced
   to forecast the run offs. An efficient disaster management system
   requires early predicted recommendations for rescue and announcement of
   exit routes in heavy runoffs. Sewages are the most complex mixture
   therefore it is not an easy task to build a model for the estimation of
   rainfall magnitude and time of non-linear runoffs occurrence. Earth can
   be regarded as a source of magnetic field known as geomagnetic field
   having range from 25 mu T to 65 mu T. This research paper elaborated the
   investigation of flash floods by observing the magnetic field lines
   using Tesla or magnetometer. Hourly based reading of magnetometer shows
   that during the runoffs the geomagnetic field briskly reduced. Results
   proved that during the run offs geomagnetic radiated by the earth got
   promptly reduced therefore geomagnetic field can be a yardstick to
   estimate the flash floods. In the 1st Hour, 2nd Hour, 3rd Hour and 4th
   Hour the geomagnetic field at the earth surface was observed 35 mu T, 25
   mu T, 25 mu T and 30 mu T. But during the runoff the geomagnetic field
   values decreased to 0.05 mu T, 0.03 mu T, 0.02 mu T and 0.02 mu T
   respectively.}},
