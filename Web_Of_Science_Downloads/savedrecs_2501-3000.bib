@inproceedings{ ISI:000452593300002,
Author = {Botavin, D. and Golosov, V. and Konoplev, A. and Wakiyama, Y.},
Book-Group-Author = {{IOP}},
Title = {{Consequences of the river valley bottom transformation after extreme
   flood (on the example of the Niida River, Japan)}},
Booktitle = {{3RD INTERNATIONAL CONFERENCE ENVIRONMENT AND SUSTAINABLE DEVELOPMENT OF
   TERRITORIES: ECOLOGICAL CHALLENGES OF THE 21ST CENTURY}},
Series = {{IOP Conference Series-Earth and Environmental Science}},
