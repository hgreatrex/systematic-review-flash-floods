@inproceedings{ ISI:000437019800014,
Author = {Fereshtehpour, Mohammad and Burian, Steven J. and Karamouz, Mohammad},
Editor = {{Kamojjala, S}},
Title = {{Flood Risk Assessments of Transportation Networks Utilizing
   Depth-Disruption Function}},
Booktitle = {{WORLD ENVIRONMENTAL AND WATER RESOURCES CONGRESS 2018: WATER,
   WASTEWATER, AND STORMWATER; URBAN WATERSHED MANAGEMENT; MUNICIPAL WATER
   INFRASTRUCTURE; AND DESALINATION AND WATER REUSE}},
Year = {{2018}},
Pages = {{134-142}},
Note = {{18th Annual World Environmental and Water Resources Congress of the
   Environmental-and-Water-Resources-Institute (EWRI), Minneapolis, MN, JUN
   03-07, 2018}},
Organization = {{Amer Soc Civil Engineers; Amer Soc Civil Engineers, Environm \& Water
   Resources Inst}},
Abstract = {{Transportation networks as a critical infrastructure are at increasing
   risk of flooding and thus damaging disruption. The resulted disruption
   even in small flood events may impose indirect damages which needs to be
   quantified to better manage traffic flows. In this paper, a framework
   was developed to address the vulnerability of transportation networks to
   pluvial and coastal flooding based on a state-of-the-art
   depth-disruption function which estimates flood depth impact on
   vehicle's speed reduction. In this framework, the flood depth was
   modeled using a static and hydrodynamic model and then assigned to each
   road segment. By turning flood depth into factors causing increase in
   travel time, the service area of four emergency services were calculated
   and the effect of coastal and pluvial flooding on road networks were
   discussed. The Lower Manhattan in New York City was selected as a case
   study. Results show that this framework provides better insights into
   determining critical road segments and subsequently traffic management
   during flooding events.}},
Publisher = {{AMER SOC CIVIL ENGINEERS}},
Address = {{UNITED ENGINEERING CENTER, 345 E 47TH ST, NEW YORK, NY 10017-2398 USA}},
Type = {{Proceedings Paper}},
