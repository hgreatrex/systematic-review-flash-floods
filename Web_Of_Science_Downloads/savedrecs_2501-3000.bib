@article{ ISI:000399358400078,
Author = {Soon, Ng Kim and Isah, Nuhu and Ali, Maimunah Binti and Ahmad, Abd
   Rahman},
Title = {{Effects of SMART Tunnel Maintenance Works on Flood Control and Traffic
   Flow}},
