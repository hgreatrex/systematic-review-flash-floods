Title = {{Social Auto-Scaling}},
Booktitle = {{2018 26TH EUROMICRO INTERNATIONAL CONFERENCE ON PARALLEL, DISTRIBUTED,
   AND NETWORK-BASED PROCESSING (PDP 2018)}},
Series = {{Euromicro Conference on Parallel Distributed and Network-Based
