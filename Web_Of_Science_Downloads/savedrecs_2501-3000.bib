@article{ ISI:000387585700008,
Author = {Haessler-Kiefhaber, Doris and Lorig, Ralf},
Title = {{Flood prevention concept for communities affected by heavy precipitation}},
Journal = {{WASSERWIRTSCHAFT}},
Year = {{2016}},
Volume = {{106}},
Number = {{11}},
Pages = {{39-42}},
Abstract = {{On 20th September 2014 the North Palatinate suffered particularly
   intense rainfall with amounts of up to 150 mm in three hours. The result
   was a flash flood of unprecedented proportions, causing damage running
   into the millions. In the hotspot communities silty water was up to 1.70
   m deep in houses as well as on farms and roads. In a pilot project
   organized by the Land Rhineland-Palatinate the experiences from this
   event were analyzed and assessed and measures developed with the aim of
   helping those affected and involved to be better prepared for future
   heavy precipitation.}},
