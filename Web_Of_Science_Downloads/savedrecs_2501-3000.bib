@article{ ISI:000430661500008,
Author = {Kvocka, Davor and Ahmadian, Reza and Falconer, Roger A.},
Title = {{Predicting Flood Hazard Indices in Torrential or Flashy River Basins and
   Catchments}},
Journal = {{WATER RESOURCES MANAGEMENT}},
Year = {{2018}},
Volume = {{32}},
Number = {{7}},
Pages = {{2335-2352}},
Month = {{MAY}},
Abstract = {{Flood hazard maps are one of the main components of any flood risk
   management strategy. It is predicted that the degree of flood risk is
   going to significantly increase in the future due to climatic and
   environmental changes, and hence it is increasingly important that
   state-of-the-art methods are implemented for assessing human stability
   in floodwaters. Therefore, this paper focuses on proposing more accurate
   and detailed guidelines for predicting flood hazard indices in small and
   steep river basins or catchments, prone to the occurrence of flash
   flooding. The results obtained in this study indicate that for river
   basins with an average bed gradient greater than 1\% (i.e. torrential or
   flashy river basins or catchments), then the flood hazard indices should
   be predicted using criteria which are based on the physical
   interpretation of the processes that affect the human stability in
   floodwaters, i.e. mechanics based and experimentally calibrated flood
   hazard assessment methods.}},
