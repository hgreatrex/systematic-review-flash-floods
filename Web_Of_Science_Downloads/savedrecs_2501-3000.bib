@inproceedings{ ISI:000343761500021,
Author = {Fu, Xudong and An, Chenge and Ma, Hongbo and Guo, Dawei and Zhang, Liguo},
Editor = {{Zhaoyin, W and Lee, JHW and Jizhang, G and Shuyou, C}},
Title = {{Flash Flood in a Mountain Stream with Pulsed Sediment Input Following an
   Earthquake: Case Study of Longxi River}},
Booktitle = {{PROCEEDINGS OF THE 35TH IAHR WORLD CONGRESS, VOLS I AND II}},
