@article{ ISI:000273969900009,
Author = {Versini, Pierre-Antoine and Gaume, Eric and Andrieu, Herve},
Title = {{Distributed hydro-meteorological simulation for a road submersion
   warning system in the Gard region (France)}},
Journal = {{HOUILLE BLANCHE-REVUE INTERNATIONALE DE L EAU}},
Year = {{2009}},
Number = {{6}},
Pages = {{72-75}},
Note = {{Meeting on Hydrometeorological Forecasts, Lyon, FRANCE, NOV 18-19, 2008}},
Abstract = {{Is it possible, during large flood events, to forecast which roads will
   be inundated to limit the damages and the number of victims and to
   identify safe routes for the rescue means ? To assess the possibilities
   and limits of the existing hydro-meteorological forecasting tools, a
   road submersion warning prototype has been developed by the Laboratoire
   Central des Ponts et Chaussees on some test areas located in the Gard
   region (France). This research corresponded to a PhD thesis {[}4]. It
   has been conducted in collaboration with the Cereve (Ecole Nationale des
   Ponts et Chaussess) and has been part of the European research project
   Floodsite (6(eme) PCRD {[}3]). The Gard region is frequently affected by
   severe flash-floods inducing road submersions. An inventory of reported
   road inundations during the last 30 years has been recently made by the
   local road management services in some part of the Gard region. It was
   therefore a particularly well suited area to conduct the first
   calibration and validation tests of the proposed prototype. The first
   results obtained are promising. They demonstrate that despite the high
   uncertainty affecting their results, distributed hydro-meteorological
   models may be of great utility for flood event management. Their
   possibilities remain largely unexplored by the flood forecasting
   services.}},
