Title = {{Analysis of Regional Development in Connection with Multi Disaster
   Susceptibility in Bengkulu Province}},
Booktitle = {{RESILIENT CITIES: BEYOND MITIGATION, PREPAREDNESS, RESPONSE, AND
   RECOVERY (CITIES 2013 INTERNATIONAL SEMINAR)}},
Series = {{Procedia Social and Behavioral Sciences}},
