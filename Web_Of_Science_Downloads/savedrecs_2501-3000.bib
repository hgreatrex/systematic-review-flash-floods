@article{ ISI:000454214100003,
Author = {Rampersadh, Akhil and Bordy, Emese M. and Sciscio, Lara and Abrahams,
   Miengah},
Title = {{DINOSAUR BEHAVIOUR IN AN EARLY JURASSIC PALAEOECOSYSTEM - UPPERMOST
