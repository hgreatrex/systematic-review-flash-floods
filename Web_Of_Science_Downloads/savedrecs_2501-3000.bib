@inproceedings{ ISI:000231697300038,
Author = {Tan, CW and Chiu, DM and Lui, JCS and Yau, DKY},
Book-Group-Author = {{IEEE Computer Society}},
Title = {{Handling high-bandwidth traffic aggregates by receiver-driven feedback
   control}},
