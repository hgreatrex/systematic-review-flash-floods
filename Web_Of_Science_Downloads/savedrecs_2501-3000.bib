@inproceedings{ ISI:000392270100199,
Author = {Candela, Angela and Aronica, Giuseppe Tito},
Editor = {{Lang, M and Klijn, F and Samuels, P}},
Title = {{Rainfall thresholds derivation for warning pluvial flooding risk in
   urbanised areas}},
Booktitle = {{3RD EUROPEAN CONFERENCE ON FLOOD RISK MANAGEMENT (FLOODRISK 2016)}},
Series = {{E3S Web of Conferences}},
