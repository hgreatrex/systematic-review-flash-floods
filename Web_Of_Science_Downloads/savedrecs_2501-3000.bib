@inproceedings{ ISI:000416965000152,
Author = {Zakaria, Siti Fairus and Zin, Rosli Mohamad and Mohamad, Ismail and
   Balubaid, Saeed and Mydin, Shaik Hussein and Roodienyanto, E. M. M. D.
   R.},
Editor = {{Saloma and Borgan, WR and Buntoro, F and Victor}},
Title = {{The Development of Flood Map in Malaysia}},
