@article{ ISI:000262008800010,
Author = {Arioli, Tagiane and Voltolini, Caroline Heinig and Santos, Marisa},
Title = {{Leaf morphoanatomy of the rheophyte Raulinoa echinata R. S. Cowan -
   Rutaceae}},
