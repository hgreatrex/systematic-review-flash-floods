@article{ ISI:000305830400027,
Author = {Mebarki, A. and Valencia, N. and Salagnac, J. L. and Barroca, B.},
Title = {{Flood hazards and masonry constructions: a probabilistic framework for
   damage, risk and resilience at urban scale}},
Journal = {{NATURAL HAZARDS AND EARTH SYSTEM SCIENCES}},
