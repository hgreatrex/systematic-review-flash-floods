@article{ ISI:000257535200005,
Author = {de Waele, Jo},
Title = {{Interaction between a dam site and karst springs: The case of Supramonte
   (Central-East Sardinia, Italy)}},
Journal = {{ENGINEERING GEOLOGY}},
Year = {{2008}},
Volume = {{99}},
