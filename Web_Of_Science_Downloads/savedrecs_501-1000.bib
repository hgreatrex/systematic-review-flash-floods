@article{ ISI:000369520900015,
Author = {Rahmati, Omid and Haghizadeh, Ali and Stefanidis, Stefanos},
Title = {{Assessing the Accuracy of GIS-Based Analytical Hierarchy Process for
   Watershed Prioritization; Gorganrood River Basin, Iran}},
Journal = {{WATER RESOURCES MANAGEMENT}},
