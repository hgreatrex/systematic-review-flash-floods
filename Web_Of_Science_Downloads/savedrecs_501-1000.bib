@article{ ISI:000386425100005,
Author = {Merheb, Mohammad and Moussa, Roger and Abdallah, Chadi and Colin,
   Francois and Perrin, Charles and Baghdadi, Nicolas},
Title = {{Hydrological response characteristics of Mediterranean catchments at
   different time scales: a meta-analysis}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{2016}},
Volume = {{61}},
Number = {{14}},
Pages = {{2520-2539}},
Abstract = {{This work examines 140 hydrological studies conducted in the
   Mediterranean region. It identifies key characteristics of the
   hydrological responses of Mediterranean catchments at various time
   scales and compares different methods and modelling approaches used for
   individual-catchment studies. The study area is divided into the
   northwestern (NWM), eastern (EM) and southern (SM) Mediterranean. The
   analysis indicates regional discrepancies in which the NWM shows the
   most extreme rainfall regime. A tendency for reduced water resources
   driven by both anthropogenic and climatic pressures and a more extreme
   rainfall regime are also noticeable. Catchments show very heterogeneous
   responses over time and space, resulting in limitations in hydrological
   modelling and large uncertainties in predictions. However, few models
   have been developed to address these issues. Additional studies are
   necessary to improve the knowledge of Mediterranean hydrological
   features and to account for regional specificities.}},
