@article{ ISI:000370447300005,
Author = {Dean, David J. and Topping, David J. and Schmidt, John C. and Griffiths,
   Ronald E. and Sabol, Thomas A.},
Title = {{Sediment supply versus local hydraulic controls on sediment transport
   and storage in a river with large sediment loads}},
Journal = {{JOURNAL OF GEOPHYSICAL RESEARCH-EARTH SURFACE}},
