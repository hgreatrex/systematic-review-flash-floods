@article{ ISI:000316560700014,
Author = {Villarini, Gabriele and Smith, James A. and Baeck, Mary Lynn and Smith,
   Brianne K. and Sturdevant-Rees, Paula},
Title = {{Hydrologic Analyses of the July 17-18, 1996, Flood in Chicago and the
   Role of Urbanization}},
