@article{ ISI:000455415800030,
Author = {Blau, Marie Luise and Luz, Frieder and Panagopoulos, Thomas},
Title = {{Urban River Recovery Inspired by Nature-Based Solutions and Biophilic
   Design in Albufeira, Portugal}},
Journal = {{LAND}},
Year = {{2018}},
Volume = {{7}},
Number = {{4}},
Month = {{DEC}},
Abstract = {{Mass urbanisation presents one of the most urgent challenges of the 21st
   century. The development of cities and the related increasing ground
   sealing are asking even more for the restoration of urban rivers,
   especially in the face of climate change and its consequences. This
   paper aims to demonstrate nature-inspired solutions in a recovery of a
   Southern European river that was canalised and transformed in culvert
   pipes. The river restoration project naturally tells the history of the
   city, creates a sense for the place, as well as unifying blue-green
   infrastructure in a symbolic way by offering areas for recreation. To
   improve well-being and city resilience in the long term, a regenerative
   sustainability approach based on biophilic design patterns was proposed.
   Such actions will provide greater health, social cohesion, and
   well-being for residents and simultaneously reduce the risks of climate
   change, such as heat island effect and flash floods, presenting the
   benefits of the transition to a regenerative economy and holistic
   thinking.}},
