@article{ ISI:000270385700005,
Author = {Kumar, Odury S. R. U. Bhanu and Rao, Sai Ramalingeswara and Krishna,
   Kailasam Muni},
Title = {{Role of cumulus parameterization schemes in simulating heavy rainfall
   episodes off the coast of Maharashtra state during 28 June-4 July 2007}},
