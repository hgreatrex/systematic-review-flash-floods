@article{ ISI:000393558900011,
Author = {Bovolin, Vittorio and Cuomo, Albina and Guida, Domenico},
Title = {{Hydraulic modeling of flood pulses in the Middle Bussento Karst System
   (MBSKS), UNESCO Cilento Global Geopark, southern Italy}},
Journal = {{HYDROLOGICAL PROCESSES}},
