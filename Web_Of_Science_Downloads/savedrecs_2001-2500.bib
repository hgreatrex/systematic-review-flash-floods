@article{ ISI:000251751600008,
Author = {Choi, H. and Kang, I. J. and Chung, Y. H. and Yoon, H. J.},
Title = {{Uncertainty analysis of flash-flood system using remote sensing and a
   geographic information system based on GcIUH in the Yeongdeok Basin,
   Korea}},
Journal = {{INTERNATIONAL JOURNAL OF REMOTE SENSING}},
Year = {{2007}},
Volume = {{28}},
Number = {{24}},
Pages = {{5551-5565}},
Abstract = {{The current paper focuses on minimizing flood damage in the Yeongdeok
   basin of South Korea by establishing a flood prediction model based on a
   geographic information system (GIS), remote sensing and
   geomorphoclimatic instantaneous unit hydrograph (GcIUH) techniques. The
   GIS database for flash flood prediction was created using data from
   digital elevation models (DEMs), soil maps and Landsat satellite
   imagery. Flood prediction was based on the peak discharge calculated at
   the sub-basin scale using hydrogeomorphologic techniques and the
   threshold runoff value. Using the developed flash flood prediction
   model, rainfall conditions with the potential to cause flooding were
   determined based on the cumulative rainfall for 20 min, considering
   rainfall duration, peak discharge and flooding in the Yeongdeok basin.}},
