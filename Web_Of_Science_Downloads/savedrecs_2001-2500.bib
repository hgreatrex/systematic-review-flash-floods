@article{ ISI:000287174400005,
Author = {Pandita, S. K. and Bhat, S. K. and Kotwal, S. S.},
Title = {{Facies Evaluation of Boulder Conglomerate Formation, Upper Siwalik,
   Jammu Himalaya}},
Journal = {{HIMALAYAN GEOLOGY}},
Year = {{2011}},
Volume = {{32}},
Number = {{1}},
Pages = {{63-69}},
Abstract = {{Various lithofacies from Boulder Conglomerate Formation of the Upper
   Siwalik Subgroup exposed around village Kangar in Jammu District are
   described to understand the depositional processes. The sedimentation
   pattern in this study displays upward changes in the facies and
   architectural elements from crudely stratified, lenticular conglomerates
   with sandstones towards the basal part of the section to thickly bedded
   disorganized conglomerate towards the top of the section. The
   characteristic features in the sedimentation pattern indicate that the
   sediments were deposited rapidly in recurrent high energy environment of
   gravelly braided river system. The braided streams were wide with
   shallow to moderate depth, and rapidly aggraded during flash floods in a
   southwesterly palaeoflow direction as indicated by cross beddings and
   pebble imbrications.}},
