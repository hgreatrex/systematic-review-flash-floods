@article{ ISI:000415725600002,
Author = {Sindhanaiselvi, D. and Shanmuganantham, T.},
Title = {{DOUBLE BOSS SCULPTURED DIAPHRAGM EMPLOYED PIEZORESISTIVE MEMS PRESSURE
   SENSOR WITH SILICON-ON-INSULATOR (SOI)}},
Journal = {{JOURNAL OF ENGINEERING SCIENCE AND TECHNOLOGY}},
