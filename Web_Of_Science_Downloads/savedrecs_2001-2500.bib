@article{ ISI:000249848500007,
Author = {Degenhardt, John and Giardino, John and Marston, Richard and Pitty,
   Alistair},
Title = {{Possible origin of the block stream in Tom Mays canyon, Franklin
   Mountains, TX}},
Journal = {{ZEITSCHRIFT FUR GEOMORPHOLOGIE}},
