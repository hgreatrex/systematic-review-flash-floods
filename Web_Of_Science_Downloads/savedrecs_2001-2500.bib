@article{ ISI:000386425400007,
Author = {Hajian, Farhad and Dykes, Alan P. and Zahabiyoun, Bagher and Ibsen, Maia},
Title = {{Prediction of climate change effects on the runoff regime of a forested
   catchment in northern Iran}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{2016}},
Volume = {{61}},
Number = {{15}},
Pages = {{2729-2739}},
Month = {{DEC}},
Abstract = {{The southern coast of the Caspian Sea in northern Iran is bordered by a
   mountain range with forested catchments which are susceptible to
   droughts and floods. This paper examines possible changes to runoff
   patterns from one of these catchments in response to climate change
   scenarios. The HEC-HMS rainfall-runoff model was used with downscaled
   future rainfall and temperature data from 13 global circulation models,
   and meteorological and hydrometrical data from the Casilian (or
   Kassilian) Catchment. Annual and seasonal predictions of runoff change
   for three future emissions scenarios were obtained, which suggest
   significantly higher spring rainfall with increased risk of flooding and
   significantly lower summer rainfall leading to a higher probability of
   drought. Flash floods arising from extreme rainfall may become more
   frequent, occurring at any time of year. These findings indicate a need
   for strategic planning of water resource management and mitigation
   measures for increasing flood hazards.}},
