@article{ ISI:000425726300003,
Author = {Krusic, Jelka and Marjanovic, Milos and Samardzic-Petrovic, Mileva and
   Abolmasov, Biljana and Andrejev, Katarina and Miladinovic, Aleksandar},
Title = {{Comparison of expert, deterministic and Machine Learning approach for
   landslide susceptibility assessment in Ljubovija Municipality, Serbia}},
Journal = {{GEOFIZIKA}},
