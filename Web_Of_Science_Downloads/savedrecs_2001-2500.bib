@article{ ISI:000308060100012,
Author = {Behrangi, Ali and Sorooshian, Soroosh and Hsu, Kuo-lin},
Title = {{Summertime evaluation of REFAME over the Unites States for near
   real-time high resolution precipitation estimation}},
