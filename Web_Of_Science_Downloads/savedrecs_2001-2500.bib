@article{ ISI:000259046300002,
Author = {Marx, Andreas and Mast, Markus and Knoche, Richard and Kunstmann, Harald},
Title = {{Global climate change and regional impact on the water balance - Case
   study in the German alpine area}},
Journal = {{WASSERWIRTSCHAFT}},
Year = {{2008}},
Volume = {{98}},
Number = {{9}},
Pages = {{12-16}},
Abstract = {{A quantification and possible effects of climate change on the water
   cycle for a German alpine and pre-alpine region have been performed
   using regional climate simulations. It showed out that temperature rises
   around 3 degrees C until the end of this century with a maximum in the
   mountainous area. Mean precipitation only slightly increases. The
   intensities of hourly precipitation events increase dramatically which
   involve more intense small scale flash flood events. Daily precipitation
   sums also rise in future times which causes mainly large scale flooding
   events. For the summer months June to August, a 30 \% reduction of
   precipitation was simulated. In combination with increasing
   temperatures, the total amount of usable water is expected to reduce.
   The risk of droughts also increases due to longer lasting and more
   intense dry periods.}},
