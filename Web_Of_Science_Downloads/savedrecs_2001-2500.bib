@inproceedings{ ISI:000308718400001,
Author = {Kreibich, H. and Dimitrova, B.},
Editor = {{DeWrachien, D and Proverbs, D and Brebbia, CA and Mambretti, S}},
Title = {{Assessment of damages caused by different flood types}},
