@inproceedings{ ISI:000391006000031,
Author = {Lohpaisankrit, Worapong and Meon, Guenter and Tingsanchali, Tawatchai},
Editor = {{Schumann, AH and Bloschl, G and Castellarin, A and Dietrich, J and Grimaldi, S and Haberlandt, U and Montanari, A and Rosbjerg, D and Viglione, A and Vorogushyn, S}},
Title = {{A framework of integrated hydrological and hydrodynamic models using
   synthetic rainfall for flash flood hazard mapping of ungauged catchments
   in tropical zones}},
Booktitle = {{SPATIAL DIMENSIONS OF WATER MANAGEMENT - REDISTRIBUTION OF BENEFITS AND
   RISKS}},
