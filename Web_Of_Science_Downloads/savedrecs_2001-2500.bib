@article{ ISI:000433498000086,
Author = {Mihai, Florin-Constantin},
Title = {{Rural plastic emissions into the largest mountain lake of the Eastern
   Carpathians}},
Journal = {{ROYAL SOCIETY OPEN SCIENCE}},
