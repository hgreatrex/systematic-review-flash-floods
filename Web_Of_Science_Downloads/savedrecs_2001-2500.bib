@inproceedings{ ISI:000365023000008,
Author = {Darras, T. and Raynaud, F. and Estupina, V. Borrell and Kong-A-Siou, L.
   and Van-Exter, S. and Vayssade, B. and Johannet, A. and Pistre, S.},
Editor = {{Cudennec, C and Eicker, A and Pilon, P and Stoffel, M and Viglione, A and Xu, Z}},
Title = {{Neural network modeling and geochemical water analyses to understand and
   forecast karst and non-karst part of flash floods (case study on the Lez
   river, Southern France)}},
Booktitle = {{EXTREME HYDROLOGICAL EVENTS}},
Series = {{Proceedings of the International Association of Hydrological Sciences
   (IAHS)}},
Year = {{2015}},
Volume = {{369}},
Pages = {{43-48}},
Note = {{Joint Inter-Association Symposium on Extreme Hydrological Events,
   Prague, CZECH REPUBLIC, JUN 22-JUL 02, 2015}},
Abstract = {{Flash floods forecasting in the Mediterranean area is a major economic
   and societal issue. Specifically, considering karst basins,
   heterogeneous structure and nonlinear behaviour make the flash flood
   forecasting very difficult. In this context, this work proposes a
   methodology to estimate the contribution from karst and non-karst
   components using toolbox including neural networks and various
   hydrological methods. The chosen case study is the flash flooding of the
   Lez river, known for his complex behaviour and huge stakes, at the gauge
   station of Lavallette, upstream of Montpellier (400 000 inhabitants).
   After application of the proposed methodology, discharge at the station
   of Lavalette is spited between hydrographs of karst flood and surface
   runoff, for the two events of 2014. Generalizing the method to future
   events will allow designing forecasting models specifically for karst
   and surface flood increasing by this way the reliability of the
   forecasts.}},
