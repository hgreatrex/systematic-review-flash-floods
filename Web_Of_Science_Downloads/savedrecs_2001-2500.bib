Title = {{Forecasting and mitigation of flooding in a Mediterranean karstic
   watershed}},
Booktitle = {{HYDROLOGY IN A CHANGING WORLD: ENVIRONMENTAL AND HUMAN DIMENSIONS}},
Series = {{IAHS Publication}},
Year = {{2014}},
Volume = {{363}},
