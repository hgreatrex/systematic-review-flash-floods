@inproceedings{ ISI:000260480900059,
Author = {Su Guanlu and Xu Shufeng and Fu Ping},
Editor = {{Shang, HQ}},
Title = {{Discussion on Measures to Regulate Dangerous Works of Bank Failure of
   Yihe and Shuhe Rivers}},
Booktitle = {{PROCEEDINGS OF THE 1ST INTERNATIONAL YELLOW RIVER FORUM ON RIVER BASIN
   MANAGEMENT, VOL II}},
Year = {{2003}},
Pages = {{474-477}},
Note = {{1st International Yellow River Forum, Zhengzhou, PEOPLES R CHINA, MAY
   12-15, 2003}},
Organization = {{Yellow River Conservancy Commiss; Minist Water Resources; China Int Ctr
   Econ \& Tech Exchange; Xiaolangdi Multiple Purpose Project Construct \&
   Adm Bur; Natl Nat Sci Fdn China; NPC, Legislat Dept Nat \& Environm
   Resources Comm}},
Abstract = {{Yihe and Shuhe Rivers are two big flash flood rivers in the Yi - Shu -
   Si liver system which flow through 14 counties ( cities, districts) in
   Shandong and Jiangsu provinces, with a drainage area of 21,070 km(2). At
   the upper reaches of the rivers is the Yimeng Mountain Area, where stand
   chain of mountains and rivers flow in valleys. At the lower reaches, are
   alluvial plains, where the river channels are flat, though the cross
   section is large, yet the flood carrying capacity is poor, and the
   foreland has very poor erosion resisting capacity. Because of the steep
   slope of the river bed at the upper reaches, many tributaries but with
   short river channels, and large water catchment area, so the characters
   of the flood in Yihe and Shuhe Rivers, such as short distance to the
   source and rapid flow, high bomb and large volume, sudden rising and
   sudden falling are formed. Whenever the flood season comes, flash floods
   with sediment rush down the mountains, scour right and left in the
   winding river channels, cause the foreland to slough, and form many
   dangerous works of bank failure, which directly threaten the safety of
   the levees and the villages along the rivers. In order to stabilize the
   river bed, protect the levees and foreland, it is urgently necessary to
   harness the sections of dangerous works.}},
