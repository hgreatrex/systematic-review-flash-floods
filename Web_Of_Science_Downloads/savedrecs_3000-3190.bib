@inproceedings{ ISI:000189482200018,
Author = {Gourley, JJ and Vieux, BE},
Editor = {{Tachikawa, Y and Vieux, BE and Georgakakos, KP and Nakakita, E}},
Title = {{The effects of radar-derived rainfall uncertainties on forecasts from a
   distributed hydrological model}},
