Title = {{Novel in-situ method for fast determination of bridge pier displacements
   during push-over tests}},
Booktitle = {{INTERNATIONAL SYMPOSIUM ON LIDAR AND RADAR MAPPING 2011: TECHNOLOGIES
   AND APPLICATIONS}},
Series = {{Proceedings of SPIE}},
Year = {{2011}},
Volume = {{8286}},
