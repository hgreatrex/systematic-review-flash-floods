Pages = {{873-886}},
Note = {{11th International Multidisciplinary Scientific GeoConference, Albena,
   BULGARIA, JUN 20-25, 2011}},
Organization = {{Minist Env \& Water; Bulgarian Acad Sci; Acad Sci Czech Republ; Acad Sci
