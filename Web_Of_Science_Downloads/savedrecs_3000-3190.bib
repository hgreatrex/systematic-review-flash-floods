Title = {{Quantitative landslide risk assessment of cairns, Australia}},
Booktitle = {{GEOTECHNICAL ENGINEERING MEETING SOCIETY'S NEEDS, VOLS 1 AND 2,
   PROCEEDINGS}},
Year = {{2001}},
Pages = {{149-154}},
Note = {{14th Southeast Asian Geotechnical Conference, HONG KONG, PEOPLES R
