@article{ ISI:000208506100009,
Author = {Cai, Hubo and Rasdorf, William and Tilley, Chris},
Title = {{Approach to Determine Extent and Depth of Highway Flooding}},
Journal = {{JOURNAL OF INFRASTRUCTURE SYSTEMS}},
