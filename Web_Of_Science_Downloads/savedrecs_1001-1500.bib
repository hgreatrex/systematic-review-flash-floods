@article{ ISI:000342297000003,
Author = {Hu, Haibo},
Title = {{An algorithm for converting weather radar data into GIS polygons and its
   application in severe weather warning systems}},
Journal = {{INTERNATIONAL JOURNAL OF GEOGRAPHICAL INFORMATION SCIENCE}},
