@article{ ISI:000330536600007,
Author = {Casteel, Mark A. and Downing, Joe R.},
Title = {{How Individuals Process NWS Weather Warning Messages on Their Cell
   Phones}},
Journal = {{WEATHER CLIMATE AND SOCIETY}},
