@article{ ISI:000325752100083,
Author = {Burg, Avihu and Zilberbrand, Michael and Yechieli, Yoseph},
Title = {{RADIOCARBON VARIABILITY IN GROUNDWATER IN AN EXTREMELY ARID ZONE-THE
   ARAVA VALLEY, ISRAEL}},
Journal = {{RADIOCARBON}},
