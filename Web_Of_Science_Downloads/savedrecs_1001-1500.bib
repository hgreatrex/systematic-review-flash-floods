@inproceedings{ ISI:000360801900007,
Author = {Abdelkader, Mohamed and Shaqura, Mohammad and Ghommem, Mehdi and
   Collier, Nathan and Calo, Victor and Claudel, Christian},
Book-Group-Author = {{IEEE}},
Title = {{Optimal multi-agent path planning for fast inverse modeling in UAV-based
   flood sensing applications}},
Booktitle = {{2014 INTERNATIONAL CONFERENCE ON UNMANNED AIRCRAFT SYSTEMS (ICUAS)}},
Series = {{International Conference on Unmanned Aircraft Systems}},
Year = {{2014}},
Pages = {{64-71}},
Note = {{International Conference on Unmanned Aircraft Systems (ICUAS), Orlando,
   FL, MAY 27-30, 2014}},
Organization = {{IEEE Robot \& Automat Soc; IEEE CSS; ICUAS; Mediterranean Control Assoc;
   Univ Denver}},
Abstract = {{Floods are the most common natural disasters, causing thousands of
   casualties every year in the world. In particular, flash flood events
   are particularly deadly because of the short timescales on which they
   occur. Unmanned air vehicles equipped with mobile microsensors could be
   capable of sensing flash floods in real time, saving lives and greatly
   improving the efficiency of the emergency response. However, of the main
   issues arising with sensing floods is the difficulty of planning the
   path of the sensing agents in advance so as to obtain meaningful data as
   fast as possible. In this particle, we present a fast numerical scheme
   to quickly compute the trajectories of a set of UAVs in order to
   maximize the accuracy of model parameter estimation over a time horizon.
   Simulation results are presented, a preliminary testbed is briefly
   described, and future research directions and problems are discussed.}},
