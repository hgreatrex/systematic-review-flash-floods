@article{ ISI:000383613700013,
Author = {Rahmani, Vahid and Hutchinson, Stacy L. and Harrington, Jr., John A. and
   Hutchinson, J. M. Shawn},
Title = {{Analysis of frequency and magnitude of extreme rainfall events with
   potential impacts on flooding: a case study from the central United
   States}},
