@article{ ISI:000287095700001,
Author = {Ahmed, Ayman A. and Fawzi, Ahmed},
Title = {{Meandering and bank erosion of the River Nile and its environmental
   impact on the area between Sohag and El-Minia, Egypt}},
Journal = {{ARABIAN JOURNAL OF GEOSCIENCES}},
