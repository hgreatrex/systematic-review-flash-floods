@article{ ISI:000289425700010,
Author = {Wan, Qilin and Xu, Jianjun},
Title = {{A numerical study of the rainstorm characteristics of the June 2005
   flash flood with WRF/GSI data assimilation system over south-east China}},
Journal = {{HYDROLOGICAL PROCESSES}},
