@article{ ISI:000335827100052,
Author = {Ivancan-Picek, Branka and Horvath, Kristian and Mahovic, Natasa Strelec
   and Gajic-Capka, Marjana},
Title = {{Forcing mechanisms of a heavy precipitation event in the southeastern
   Adriatic area}},
