@article{ ISI:000284785800009,
Author = {Balasch, J. C. and Ruiz-Bellet, J. L. and Tuset, J. and Martin de Oliva,
   J.},
Title = {{Reconstruction of the 1874 Santa Tecla's rainstorm in Western Catalonia
   (NE Spain) from flood marks and historical accounts}},
Journal = {{NATURAL HAZARDS AND EARTH SYSTEM SCIENCES}},
Year = {{2010}},
Volume = {{10}},
Number = {{11}},
Pages = {{2317-2325}},
Abstract = {{The Santa Tecla flash flood, a very heavy event occurred in western
   Catalonia (NE Spain) in 1874, was reconstructed with hydraulic and
   hydrological modelling tools in three basins. The hydrograph obtained in
   a first step and the basin soil moisture information ultimately allowed
   the estimation of the range of the rainstorm magnitude which caused the
   flash flood. The reconstruction of historical floods has proved useful
   to improve the flood probability analysis, especially in ungauged
   basins.}},
