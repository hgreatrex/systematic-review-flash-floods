@article{ ISI:000385435200008,
Author = {Nielsen, Erik R. and Schumacher, Russ S.},
Title = {{Using Convection-Allowing Ensembles to Understand the Predictability of
   an Extreme Rainfall Event}},
Journal = {{MONTHLY WEATHER REVIEW}},
