@article{ ISI:000316199100030,
Author = {Yousif, Mohamed and Bubenzer, Olaf},
Title = {{Integrated remote sensing and GIS for surface water development. Case
   study: Ras El Hekma area, northwestern coast of Egypt}},
Journal = {{ARABIAN JOURNAL OF GEOSCIENCES}},
