@article{ ISI:000259482900007,
Author = {Chen Chien-Yuan and Chen Lien-Kuang and Yu Fan-Chieh and Lin Sheng-Chi
   and Lin Yu-Ching and Lee Chou-Lung and Wang Yu-Ting and Cheung Kei-Wai},
Title = {{Characteristics analysis for the flash flood-induced debris flows}},
Journal = {{NATURAL HAZARDS}},
Year = {{2008}},
Volume = {{47}},
Number = {{2}},
Pages = {{245-261}},
Month = {{NOV}},
Abstract = {{Typhoon Haitang caused landfall on Taiwan during 15-21 July, 2005 and
   brought 2,279 mm of maximum cumulative rain with a maximum intensity of
   176 mm/ h. The torrential rain was mainly distributed from the central
   mountain range to southern Taiwan and triggered 222 slopeland-related
   hazards. Among the hazard events, there were 17 debris flows, 157 cases
   of traffic cut-off, three large-magnitude deep-seated landslides, and 10
   villages isolated in the off-track mountainous areas. The debris flows
   initiated in southern Taiwan were associated with torrential rain, short
   channel length (<2 km), and small basin area (<3 km(2)), and were
   speculated to be induced by flash flood. These flash flood-induced
   debris flows have a higher rainfall intensity-duration threshold for
   initiation than in other areas. The deep-seated landslides, isolated
   villages due to traffic cut-off in off-track mountain areas, and
   recurrent hazards in areas affected by the ML 7.6 Chi-Chi earthquake in
   1999 are characteristics of slopeland hazards in Taiwan in recent years.
   One of the most urgently needed mitigation strategies in response to
   slopeland hazards is the plan for enhancing self-rescue disaster
   resistance in off-track mountainous villages in Taiwan.}},
