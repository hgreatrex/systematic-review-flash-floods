@article{ ISI:000494280000052,
Author = {Wang, Haofang and Yun, Ruan and Zhao, Ranhang and Qi, Zhen},
Title = {{Flash flood risk evaluation based on variable fuzzy method and fuzzy
   clustering analysis}},
