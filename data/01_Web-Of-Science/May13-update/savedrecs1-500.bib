@article{ ISI:000498708900002,
Author = {Gonzalez-Cao, Jose and Garcia-Feal, Orlando and Fernandez-Novoa, Diego
   and Manuel Dominguez-Alonso, Jose and Gomez-Gesteira, Moncho},
Title = {{Towards an automatic early warning system of flood hazards based on
   precipitation forecast: the case of the Mino River (NW Spain)}},
Journal = {{NATURAL HAZARDS AND EARTH SYSTEM SCIENCES}},
Year = {{2019}},
Volume = {{19}},
Number = {{11}},
Pages = {{2583-2595}},
Month = {{NOV 20}},
Abstract = {{An early warning system for flood prediction based on precipitation
   forecast is presented. The system uses rainfall forecast provided by
   MeteoGalicia in combination with a hydrologic (Hydrologic Modeling
   System, HEC-HMS) and a hydraulic (Iber+) model. The upper reach of the
   Mino River and the city of Lugo (NW Spain) are used as a study area.
   Starting from rainfall forecast, HEC-HMS calculates the streamflow and
   Iber+ is automatically executed for some previously defined risk areas
   when a certain threshold is exceeded. The analysis based on historical
   extreme events shows that the system can provide accurate results in
   less than 1 h for a forecast horizon of 3 d and report an alert
   situation to decision makers.}},
