@article{ ISI:000489938700003,
Author = {Dhote, Pankaj R. and Aggarwal, S. P. and Thakur, Praveen K. and Garg,
   Vaibhav},
Title = {{Flood inundation prediction for extreme flood events: a case study of
   Tirthan River, North West Himalaya}},
Journal = {{HIMALAYAN GEOLOGY}},
Year = {{2019}},
Volume = {{40}},
Number = {{2}},
Pages = {{128-140}},
Abstract = {{This paper is aimed to generate flood inundation maps for the
   flood-prone stretch of Tirthan River, Himachal Pradesh, India by
   establishing hydrodynamic models (HD). An attempt has been made to
   investigate the capability of 1D and 1D-2D coupled HD models for flood
   inundation prediction of extreme flood events.1D model solving fully
   dynamic Saint Venant equation was used for 1D approach and modeling
   package which dynamically couples 1D and 2D model was used for 1D-2D
   coupled approach. Flood frequency analysis was carried out using
   historical records for estimation of extreme flood event discharge
   magnitudes. Stable (calibrated and validated) models were simulated for
   flood inundation prediction with extreme even discharge value as a
   boundary condition. It was concluded that 1D approach gives efficient
   results in the mountainous terrain of Tirthan River while river
   channel-floodplain characteristics restrict the application of 1D-2D
   coupled model.}},
