@article{ ISI:000472617500010,
Author = {Tudorache, Andreea-Violeta},
Title = {{EXTREME VARIABILITIES OF RAINFALL AND RUNOFF REGIME WITHIN VULNERABLE
   HYDROGRAPHIC RIVER BASINS. CASE STUDY: ELAN RIVER BASIN, 2016 (ROMANIA)}},
Journal = {{PRESENT ENVIRONMENT AND SUSTAINABLE DEVELOPMENT}},
Year = {{2019}},
Volume = {{13}},
Number = {{1}},
Pages = {{129-136}},
Month = {{JUN}},
Abstract = {{The present paper analyzes the extreme variabilities of rainfall and
   runoff regime within vulnerable hydrographic river basins, focused on a
   case study: Elan river basin, year 2016. This year, due to excess
   rainfall, the Elan River basin was affected by torrential rainfall,
   warned against by orange and red code hydrological forecasts. For this
   reason, this study makes an analysis of the spatial and temporal
   variability of the surface runoff also considering the main flood events
   occurring in this river basin. The impact of liquid precipitation on the
   surface runoff will be highlighted by a statistical analysis of the
   relationship between monthly average flows and the sum of monthly
   precipitation in the river basin. The monthly flows series from the
   Murgeni and Posta Elan hydrometric stations were capitalized through
   appropriate statistical analyses. Maximum flows were reported to the
   thresholds values corresponding to the Defense Levels.}},
