@article{ ISI:000432329700003,
Author = {Liu, Changjun and Guo, Liang and Ye, Lei and Zhang, Shunfu and Zhao,
   Yanzeng and Song, Tianyu},
Title = {{A review of advances in China's flash flood early-warning system}},
