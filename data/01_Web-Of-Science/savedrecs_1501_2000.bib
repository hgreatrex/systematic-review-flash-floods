@article{ ISI:000370068400025,
Author = {Werren, Gabriela and Reynard, Emmanuel and Lane, Stuart N. and Balin,
   Daniela},
Title = {{Flood hazard assessment and mapping in semi-arid piedmont areas: a case
   study in Beni Mellal, Morocco}},
Journal = {{NATURAL HAZARDS}},
Year = {{2016}},
Volume = {{81}},
Number = {{1}},
Pages = {{481-511}},
Month = {{MAR}},
Abstract = {{Flood hazard assessment and mapping is challenging in semi-arid ungauged
   basins because of the lack of data, the rapid runoff response, climatic
   variability, and the difficulties of modelling hydraulic processes, such
   as on piedmont alluvial fans. This study combines hydrological and
   hydraulic modelling with hydro-geomorphic knowledge gathered during
   post-flood campaigns in order to determine realistic flood hazard
   scenarios for a piedmont urban area in Morocco. Partial calibration of
   the hydrological and hydraulic models is performed using field-estimated
   peak discharge values and mapped flood extents for known flood events.
   The calibrated models are then applied to lower frequency-higher
   magnitude flood scenarios. Finally, a flood hazard map is designed using
   the Swiss hazard assessment and mapping procedures.}},
Publisher = {{SPRINGER}},
Address = {{233 SPRING ST, NEW YORK, NY 10013 USA}},
Type = {{Article}},
Language = {{English}},
Affiliation = {{Werren, G (Reprint Author), Univ Lausanne, Lausanne, Switzerland.
   Werren, Gabriela; Reynard, Emmanuel; Lane, Stuart N.; Balin, Daniela, Univ Lausanne, Lausanne, Switzerland.}},
DOI = {{10.1007/s11069-015-2092-0}},
ISSN = {{0921-030X}},
EISSN = {{1573-0840}},
Keywords = {{Semi-arid catchments; Hydro-geomorphic mapping; Post-flood
