@article{ ISI:000316260100009,
Author = {Hegedues, Peter and Czigany, Szabolcs and Balatonyi, Laszlo and
   Pirkhoffer, Ervin},
Title = {{Analysis of soil boundary conditions of flash floods in a small basin in
   SW Hungary}},
