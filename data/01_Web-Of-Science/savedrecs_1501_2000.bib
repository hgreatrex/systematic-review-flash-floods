@article{ ISI:000423877400001,
Author = {Rogelis, Maria Carolina and Werner, Micha},
Title = {{Streamflow forecasts from WRF precipitation for flood early warning in
   mountain tropical areas}},
Journal = {{HYDROLOGY AND EARTH SYSTEM SCIENCES}},
