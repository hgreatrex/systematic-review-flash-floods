@inproceedings{ ISI:000404120100028,
Author = {Nuryanto, Danang Eko and Pawitan, Hidayat and Hidayat, Rahmat and
   Aldrian, Edvin},
Editor = {{Setiawan, Y}},
Title = {{Propagation of convective complex systems triggering potential flooding
   rainfall of Greater Jakarta using satellite data}},
Booktitle = {{3RD INTERNATIONAL SYMPOSIUM ON LAPAN-IPB SATELLITE FOR FOOD SECURITY AND
   ENVIRONMENTAL MONITORING 2016}},
Series = {{IOP Conference Series-Earth and Environmental Science}},
Year = {{2017}},
Volume = {{54}},
