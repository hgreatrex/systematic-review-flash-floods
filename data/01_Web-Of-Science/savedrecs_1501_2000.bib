Editor = {{Mastorakis, NE and Mladenov, V and Bojkovic, Z and Simian, D and Kartalopoulos, S and Varonides, A and Udriste, C and Kindler, E and Narayanan, S and Mauri, JL and Parsiani, H and Man, KL}},
Title = {{A Real Time Simulation and Modeling of Flood Hazard}},
