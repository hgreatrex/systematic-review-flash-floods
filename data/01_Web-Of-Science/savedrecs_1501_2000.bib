@article{ ISI:000387168800012,
Author = {Harpham, Quillon and Lhomme, Julien and Parodi, Antonio and Fiori,
   Elisabetta and Jagers, Bert and Galizia, Antonella},
Title = {{USING OPENMI AND A MODEL MAP TO INTEGRATE WATERML2 AND NETCDF DATA
   SOURCES INTO FLOOD MODELING OF GENOA, ITALY}},
Journal = {{JOURNAL OF THE AMERICAN WATER RESOURCES ASSOCIATION}},
Year = {{2016}},
