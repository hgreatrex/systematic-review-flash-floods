@article{ ISI:000419179200003,
Author = {Hachani, Sahar and Boudevillain, Brice and Delrieu, Guy and Bargaoui,
   Zoubeida},
Title = {{Drop Size Distribution Climatology in Cevennes-Vivarais Region, France}},
Journal = {{ATMOSPHERE}},
