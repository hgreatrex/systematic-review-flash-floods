@inproceedings{ ISI:000183790200028,
Author = {Flori, P and Pancaldi, D and Braschi, I and Gessa, C},
Editor = {{DelRe, AAM and Capri, E and Padovani, L and Trevisan, M}},
Title = {{Behavior of four herbicides in a paddy field: Simulation by a laboratory
   microcosm}},
Booktitle = {{PESTICIDE IN AIR, PLANT, SOIL \& WATER SYSTEM}},
Year = {{2003}},
Pages = {{223-232}},
Note = {{12th Symposium on Pesticide Chemistry, PIACENZA, ITALY, JUN 04-06, 2003}},
Abstract = {{The behaviour of commercial herbicide formulations containing
   pretilachlor, oxadiazon, cinosulfuron and azimsulfuron was studied in a
   laboratory model to investigate possible management practices for
   controlling herbicide losses from paddy fields. The model consisted of
   soil and water sampled from a North Italian paddy field.
   Flooding/draining simulations and herbicide treatments were performed on
   the paddy field model following agricultural practices for a period of
   101 days. Pretilachlor and subsequently oxadiazon were applied to the
   flooded model, while cinosulfuron and azimsulfuron were simultaneously
   applied to the partially-drained model. Soil and water samples were
   collected and analyzed in order to evaluate the persistence of each
   herbicide in water and soil and the leaching into the soil bottom layer.
   After 10 days from the treatment, the results showed an a.i.s'
   persistence in water as follows: pretilachlor (57.0\% of the initially
   applied amount) > oxadiazon (30.9\%); cinosulfuron (44.8\%) >
   azimsulfuron (39.4\%). No herbicide residues were found in deep and
   leaching water. Throughout the experiment, small amounts of pretilachlor
   (1.9-7.2\%) were detected in deep soil, whereas a larger amount of
   oxadiazon (8.5-20.4\%), was measured. Azimsulfuron was more mobile than
   cinosulfuron along the soil profile.}},
