@article{ ISI:000321864700003,
Author = {Dickerson, Robert Paul and Forman, Amy and Liu, Tanzhuo},
Title = {{Co-development of alluvial fan surfaces and arid botanical communities,
   Stonewall Flat, Nevada, USA}},
Journal = {{EARTH SURFACE PROCESSES AND LANDFORMS}},
