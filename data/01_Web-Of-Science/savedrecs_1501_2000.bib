@article{ ISI:000409304800008,
Author = {Milrad, Shawn M. and Lombardo, Kelly and Atallah, Eyad H. and Gyakum,
   John R.},
Title = {{Numerical Simulations of the 2013 Alberta Flood: Dynamics,
   Thermodynamics, and the Role of Orography}},
