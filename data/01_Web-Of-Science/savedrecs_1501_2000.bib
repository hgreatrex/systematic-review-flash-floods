@article{ ISI:000273969900003,
Author = {Marchandise, Arthur and Viel, Christian},
Title = {{Using the Meteo-France Safran-Isba-Modcou data for operational flood
   warning and forecasting}},
Journal = {{HOUILLE BLANCHE-REVUE INTERNATIONALE DE L EAU}},
Year = {{2009}},
Number = {{6}},
Pages = {{35-41}},
Note = {{Meeting on Hydrometeorological Forecasts, Lyon, FRANCE, NOV 18-19, 2008}},
Abstract = {{This paper deals with the use, for operational purposes, of the
   SAFRAN-ISBA-MODCOU data of Meteo-France by the French central
   hydrometeorological service for flood forecasting (SCHAP1). Good
   correlations -better than those carried out with an antecedent
   precipitation indexes- between the liquid watercontent of the ISBA root
   zone layer (Hu2) and even-based runoff coefficients were obtained on
   several catchments concerned by flash floods. The applications concern
   the production of a daily map of the Hu2 index. In order to make clearer
   the information, these maps refer to a monthly and annually climatology,
   which was established with 25 Years of data. Then, abacus which connect
   the initial soil moisture of the basin -mean of the Hu2 index on the
   basin-, the rainfall amounts and the predictable flow variation were
   established on a dozen of French catchments. These maps and abacus allow
   the 22 Flood French Forecasting Services and the SCHAP1 a best expertise
   for producing the French national warning map
   (www.vigicrues.ecologie.gouv.fr). Eventually the use of the Hu2 index
   for initializing event-based distributed rainfall-runoff modelling gave
   promising results.}},
