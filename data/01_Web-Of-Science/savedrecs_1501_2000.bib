@article{ ISI:000427011700019,
Author = {Gilabert, Joan and Carmen Llasat, Maria},
Title = {{Circulation weather types associated with extreme flood events in
   Northwestern Mediterranean}},
Journal = {{INTERNATIONAL JOURNAL OF CLIMATOLOGY}},
Year = {{2018}},
Volume = {{38}},
