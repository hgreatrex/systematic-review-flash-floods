@article{ ISI:000254685700028,
Author = {Elhanafy, Hossam and Copeland, Graham J. M. and Gejadze, Igor Yu.},
Title = {{Estimation of predictive uncertainties in flood wave propagation in a
   river channel using adjoint sensitivity analysis}},
Journal = {{INTERNATIONAL JOURNAL FOR NUMERICAL METHODS IN FLUIDS}},
