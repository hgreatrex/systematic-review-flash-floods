@article{ ISI:000341596300004,
Author = {Jansa, A. and Alpert, P. and Arbogast, P. and Buzzi, A. and
   Ivancan-Picek, B. and Kotroni, V. and Llasat, M. C. and Ramis, C. and
   Richard, E. and Romero, R. and Speranza, A.},
Title = {{MEDEX: a general overview}},
