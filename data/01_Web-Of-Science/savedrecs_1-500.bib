@inproceedings{ ISI:000320012300011,
Author = {Liberato, Margarida L. R. and Ramos, Alexandre M. and Trigo, Ricardo M.
   and Trigo, Isabel F. and Maria Duran-Quesada, Ana and Nieto, Raquel and
   Gimeno, Luis},
Editor = {{Lin, J and Brunner, D and Gerbig, C and Stohl, A and Luhar, A and Webley, P}},
Title = {{Moisture Sources and Large-Scale Dynamics Associated With a Flash Flood
   Event}},
Booktitle = {{LAGRANGIAN MODELING OF THE ATMOSPHERE}},
Series = {{Geophysical Monograph Book Series}},
