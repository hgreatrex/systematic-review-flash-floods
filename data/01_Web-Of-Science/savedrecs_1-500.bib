@article{ ISI:000272139500020,
Author = {Funatsu, Beatriz M. and Claud, Chantal and Chaboureau, Jean-Pierre},
Title = {{Comparison between the Large-Scale Environments of Moderate and Intense
   Precipitating Systems in the Mediterranean Region}},
