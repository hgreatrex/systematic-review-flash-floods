@inproceedings{ ISI:000188940600018,
Author = {Stading, T and Maniatis, P and Baker, M},
Editor = {{Druschel, P and Kaashoek, F and Rowstron, A}},
Title = {{Peer-to-peer caching schemes to address flash crowds}},
Booktitle = {{PEER-TO-PEER SYSTEMS}},
Series = {{LECTURE NOTES IN COMPUTER SCIENCE}},
Year = {{2002}},
Volume = {{2429}},
Pages = {{203-213}},
Note = {{1st International Workshop on Peer-to-Peer Systems, CAMBRIDGE,
   MASSACHUSETTS, MAR 07-08, 2002}},
Organization = {{Microsoft Res}},
Abstract = {{Flash crowds can cripple a web site's performance. Since they are
   infrequent and unpredictable, these floods do not justify the cost of
   traditional commercial solutions. We describe Backslash, a collaborative
   web mirroring system run by a collective of web sites that wish to
   protect themselves from flash crowds. Backslash is built on a
   distributed hash table overlay and uses the structure of the overlay to
   cache aggressively a resource that experiences an uncharacteristically
   high request load. By redirecting requests for that resource uniformly
   to the created caches, Backslash helps alleviate the effects of flash
   crowds. We explore cache diffusion techniques for use in such a system
   and find that probabilistic forwarding improves load distribution albeit
   not, dramatically.}},
