@article{ ISI:000316560700002,
Author = {Kitzmiller, David and Miller, Dennis and Fulton, Richard and Ding, Feng},
Title = {{Radar and Multisensor Precipitation Estimation Techniques in National
   Weather Service Hydrologic Operations}},
Journal = {{JOURNAL OF HYDROLOGIC ENGINEERING}},
