@article{ ISI:000316580600002,
Author = {Stensrud, David J. and Wicker, Louis J. and Xue, Ming and Dawson, II,
   Daniel T. and Yussouf, Nusrat and Wheatley, Dustan M. and Thompson,
   Therese E. and Snook, Nathan A. and Smith, Travis M. and Schenkman,
