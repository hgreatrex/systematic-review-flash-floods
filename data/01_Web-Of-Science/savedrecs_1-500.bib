@article{ ISI:000295378300016,
Author = {Roux, H. and Labat, D. and Garambois, P. -A. and Maubourguet, M. -M. and
   Chorda, J. and Dartus, D.},
Title = {{A physically-based parsimonious hydrological model for flash floods in
   Mediterranean catchments}},
