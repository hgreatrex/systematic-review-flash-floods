@article{ ISI:000267263300005,
Author = {Schumacher, Russ S.},
Title = {{Mechanisms for Quasi-Stationary Behavior in Simulated
   Heavy-Rain-Producing Convective Systems}},
Journal = {{JOURNAL OF THE ATMOSPHERIC SCIENCES}},
Year = {{2009}},
Volume = {{66}},
