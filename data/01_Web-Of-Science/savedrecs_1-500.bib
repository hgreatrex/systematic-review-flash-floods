@article{ ISI:000267743900045,
Author = {Bocheva, Lilia and Marinova, Tania and Simeonov, Petio and Gospodinov,
   Ilian},
Title = {{Variability and trends of extreme precipitation events over Bulgaria
   (1961-2005)}},
