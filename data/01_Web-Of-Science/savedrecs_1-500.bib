@article{ ISI:000291520400015,
Author = {Lecca, G. and Petitdidier, M. and Hluchy, L. and Ivanovic, M. and
   Kussul, N. and Ray, N. and Thieron, V.},
Title = {{Grid computing technology for hydrological applications}},
Journal = {{JOURNAL OF HYDROLOGY}},
