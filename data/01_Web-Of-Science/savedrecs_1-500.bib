@article{ ISI:000299955600030,
Author = {Yang, Han-Chung and Chen, Cheng-Wu},
Title = {{Potential hazard analysis from the viewpoint of flow measurement in
   large open-channel junctions}},
Journal = {{NATURAL HAZARDS}},
