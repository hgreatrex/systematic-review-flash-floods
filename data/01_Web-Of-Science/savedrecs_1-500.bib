@article{ ISI:000182434400004,
Author = {Cenderelli, DA and Wohl, EE},
Title = {{Flow hydraulics and geomorphic effects of glacial-lake outburst floods
   in the Mount Everest region, Nepal}},
Journal = {{EARTH SURFACE PROCESSES AND LANDFORMS}},
