@article{ ISI:000085996300008,
Author = {Ogden, FL and Sharif, HO and Senarath, SUS and Smith, JA and Baeck, ML
   and Richardson, JR},
Title = {{Hydrologic analysis of the Fort Collins, Colorado, flash flood of 1997}},
Journal = {{JOURNAL OF HYDROLOGY}},
